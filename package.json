{
  "name": "discord_app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build:dev": "env-cmd -f .env.development craco build -p",
    "build:prod": "env-cmd -f .env.production craco build -p",
    "build:predprod": "env-cmd -f .env.predprod craco build -p",
    "build:analyze": "craco build -p -- --env.addons=bundleanalyzer",
    "test": "jest",
    "test:update": "jest -u",
    "test:snapshot": "jest --testMatch='**/*.snapshot.test.{js,jsx,ts,tsx}'",
    "clear": "yarn cache clean && rm -rf ./node_modules/ yarn.lock && yarn",
    "eject": "react-scripts eject",
    "lint": "eslint",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \\\"src/**/*.{js,jsx,ts,tsx,css,md,json}\\\" --config ../tesr/.prettierrc"
  },
  "dependencies": {
    "@ant-design/icons-svg": "^4.2.1",
    "@fontsource/roboto": "^5.0.5",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@pbe/react-yandex-maps": "^1.2.4",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/cookie": "^0.5.1",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.32",
    "@types/react-dom": "^18.0.11",
    "@types/react-typical": "^0.1.2",
    "@types/react-window": "^1.8.5",
    "@types/styled-components": "^5.1.26",
    "antd": "^5.16.2",
    "axios": "^1.3.5",
    "cookie": "^0.5.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.0.3",
    "formik": "^2.2.9",
    "global": "^4.4.0",
    "localforage": "^1.10.0",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.29.4",
    "querystring-es3": "^0.2.1",
    "rc-segmented": "^2.2.0",
    "rc-select": "^14.4.3",
    "react": "^18.2.0",
    "react-countup": "^6.5.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-imask": "^7.3.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-input-mask": "3.0.0-alpha.2",
    "react-intersection-observer": "^9.10.3",
    "react-redux": "^8.0.5",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.10.0",
    "react-scripts": "5.0.1",
    "react-text-mask": "^5.5.0",
    "react-textarea-autosize": "^8.5.3",
    "react-to-print": "^2.14.13",
    "react-typical": "^0.1.3",
    "react-virtualized": "^9.22.5",
    "react-virtuoso": "^4.6.3",
    "react-window": "^1.8.9",
    "react-window-scroller": "1.0.10",
    "recharts": "^2.7.3",
    "redux-persist": "^6.0.0",
    "serve": "^14.2.0",
    "socket.io-client": "^4.7.5",
    "styled-components": "^5.3.9",
    "text-encoding": "^0.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.0.3",
    "undici": "^6.16.1",
